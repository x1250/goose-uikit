{
  "name": "@pancakeswap-libs/uikit",
  "version": "0.10.2",
  "description": "Set of UI components for pancake projects",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/x1250/goose-uikit",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "release": "yarn build && np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@pancakeswap-libs/eslint-config-pancake": "git+https://github.com/x1250/eslint-config-pancake.git",
    "@rollup/plugin-typescript": "^8.1.1",
    "@storybook/addon-a11y": "^6.1.16",
    "@storybook/addon-actions": "^6.1.16",
    "@storybook/addon-essentials": "^6.1.16",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/react": "^6.1.16",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.1",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-transition-group": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-styled-components": "^7.1.1",
    "np": "^8.0.4",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "rollup": "^3.29.1",
    "styled-components": "^6.0.0",
    "themeprovider-storybook": "^1.8.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0"
  },
  "dependencies": {
    "@storybook/addons": "^7.4.1",
    "@types/lodash": "^4.14.162",
    "@types/styled-system": "^5.1.10",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.0.7",
    "styled-system": "^5.1.5",
    "stylis": "^4.0.0"
  },
  "packageManager": "yarn@3.6.3"
}
